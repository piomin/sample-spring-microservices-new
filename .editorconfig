; This file is for unifying the coding style for different editors and IDEs.
; More information at http://editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.java]
indent_size = 2
indent_style = space

[*.xml]
indent_size = 2
indent_style = space

[*.md]
trim_trailing_whitespace = false
